{
  "name": "api-github-interview",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "https": "ng serve --ssl=true --sslCert=ssl/server.crt --sslKey=ssl/server.key",
    "local": "ng serve --host=0.0.0.0 --ssl=true --sslCert=ssl/server.crt --sslKey=ssl/server.key",
    "start": "ng serve",
    "debug:ios": "remotedebug_ios_webkit_adapter --port=4200",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:ssr": "ng run ApiGithubInterview:server:dev",
    "test": "ng test",
    "lint": "ng lint",
    "docker:build": "docker build --rm -t api-github-interview-website:latest -f Dockerfile .",
    "docker:start": "docker run -d -p 127.0.0.1:4000:4000 api-github-interview-website:latest",
    "docker:build:start": "npm run docker:build && npm run docker:start",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.2",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/material": "^10.2.1",
    "@angular/material-moment-adapter": "^10.2.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/platform-server": "^10.1.1",
    "@angular/router": "~10.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@sentry/browser": "^5.15.5",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "exif-js": "^2.3.0",
    "intersection-observer": "^0.11.0",
    "jquery": "^3.5.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "optionalDependencies": {
    "node-sass": "^4.13.0",
    "protractor": "^5.4.2",
    "ts-node": "^8.0.1",
    "tslint": "^5.0.0"
  }
}
